{
  "name": "@metamorph/plugin-sdk",
  "version": "1.0.0",
  "description": "METAMORPH Plugin SDK - Create plugins for web UI and CLI",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/core/index.d.ts",
        "default": "./dist/core/index.js"
      },
      "require": {
        "types": "./dist/core/index.d.cts",
        "default": "./dist/core/index.cjs"
      }
    },
    "./web": {
      "import": {
        "types": "./dist/web/index.d.ts",
        "default": "./dist/web/index.js"
      },
      "require": {
        "types": "./dist/web/index.d.cts",
        "default": "./dist/web/index.cjs"
      }
    },
    "./cli": {
      "import": {
        "types": "./dist/cli/index.d.ts",
        "default": "./dist/cli/index.js"
      },
      "require": {
        "types": "./dist/cli/index.d.cts",
        "default": "./dist/cli/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/web/hooks.d.ts",
        "default": "./dist/web/hooks.js"
      },
      "require": {
        "types": "./dist/web/hooks.d.cts",
        "default": "./dist/web/hooks.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "metamorph",
    "plugin",
    "sdk",
    "ai",
    "emotion-detection",
    "webcam",
    "tts",
    "stt"
  ],
  "author": "METAMORPH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/genecyber/NOESIS.git",
    "directory": "packages/sdk"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  }
}
