{
  "name": "metamorph",
  "version": "0.1.0",
  "description": "Transformation-maximizing AI system built on Claude Agent SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "metamorph": "./dist/cli/index.js",
    "metamorph-stream": "./dist/cli/stream.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "cli": "node dist/cli/index.js",
    "server": "node dist/server/index.js",
    "test": "vitest run --exclude '**/integration.test.ts'",
    "test:watch": "vitest --exclude '**/integration.test.ts'",
    "test:integration": "vitest run --include '**/integration*.test.ts' --include 'src/__tests__/integration/**/*.test.ts'",
    "test:all": "vitest run",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "sync": "npx tsx scripts/sync-memories.ts",
    "sync:pull": "npx tsx scripts/sync-remote.ts",
    "sync:push": "npx tsx scripts/push-to-remote.ts",
    "dedupe": "npx tsx scripts/dedupe-memories.ts"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.12",
    "@anthropic-ai/sdk": "^0.71.2",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@tensorflow/tfjs-node": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@vercel/sandbox": "^1.3.1",
    "@xenova/transformers": "^2.17.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "face-api.js": "^0.22.2",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.3",
    "node-webcam": "^0.8.0",
    "ora": "^8.0.0",
    "uuid": "^10.0.0",
    "ws": "^8.16.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "supertest": "^7.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "ai",
    "claude",
    "agent",
    "transformation",
    "metamorph"
  ],
  "license": "MIT",
  "optionalDependencies": {
    "@tailwindcss/oxide-linux-x64-gnu": "^4.1.18",
    "@tailwindcss/oxide-linux-x64-musl": "^4.1.18"
  }
}
