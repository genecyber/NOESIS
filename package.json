{
  "name": "metamorph",
  "version": "0.1.0",
  "description": "Transformation-maximizing AI system built on Claude Agent SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "metamorph": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "cli": "node dist/cli/index.js",
    "server": "node dist/server/index.js",
    "test": "vitest run --exclude '**/integration.test.ts'",
    "test:watch": "vitest --exclude '**/integration.test.ts'",
    "test:integration": "vitest run --testPathPattern integration.test.ts",
    "test:all": "vitest run",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.12",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@xenova/transformers": "^2.17.2",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "ora": "^8.0.0",
    "uuid": "^10.0.0",
    "zod": "^4.0.0",
    "face-api.js": "^0.22.2",
    "@tensorflow/tfjs-node": "^4.17.0",
    "node-webcam": "^0.8.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "supertest": "^7.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "ai",
    "claude",
    "agent",
    "transformation",
    "metamorph"
  ],
  "license": "MIT"
}
